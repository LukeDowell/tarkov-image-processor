{
    "app_name": "tarkov-inventory-tracker",
    "author": "Luke Dowell",
    "dependencies": {
        "cheerio": "^0.22.0",
        "crawler": "^1.3.0",
        "jimp": "^0.16.1",
        "opencv-ts": "^1.3.6",
        "windows-ss": "*",
        "workerpool": "^6.2.1"
    },
    "description": "Uses template matching to parse information related to Escape From Tarkov",
    "devDependencies": {
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-typescript": "^7.18.6",
        "@types/cheerio": "^0.22.31",
        "@types/crawler": "^1.2.2",
        "@types/jest": "^28.1.6",
        "@types/node": "^18.6.2",
        "@types/webpack-dev-server": "^4.7.2",
        "@types/workerpool": "^6.1.0",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "babel-loader": "^8.2.5",
        "eslint": "^8.20.0",
        "jest": "^28.1.3",
        "nw": "^0.66.1",
        "ts-jest": "^28.0.7",
        "ts-loader": "^9.3.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    },
    "license": "ISC",
    "main": "./index.html",
    "name": "tarkov-inventory-tracker",
    "nodejs": false,
    "scripts": {
        "lint": "npx eslint . --ext .ts",
        "publish": "rm -rf ./node_modules && npm i --production && node-prune",
        "scraper": "tsc && node dist/scraper/scraper.js",
        "start": "node dist/app.js",
        "start-nw": "../sdk/nwjs/nw.exe --disable-gpu-compositing --force-cpu-draw .",
        "start-nw-powershell": "tsc && \"..\\sdk\\nwjs\\nw.exe\" --disable-gpu-compositing --force-cpu-draw .",
        "test": "jest"
    },
    "version": "1.0.0",
    "webexe_settings": {
        "download_dir": "",
        "force_download": false,
        "nw_compression_level": 0,
        "nw_version": "0.66.1",
        "sdk_build": false,
        "uncompressed_folder": false,
        "windows-x32": true,
        "windows-x64": true
    },
    "webkit": {},
    "window": {
        "always_on_top": true,
        "exe_icon": "avatar.jpg",
        "frame": false,
        "fullscreen": true,
        "icon": "avatar.jpg",
        "id": "tarkov-image-processor",
        "resizable": true,
        "show": true,
        "show_in_taskbar": true,
        "title": "Tarkov Inventory Tracker",
        "transparent": true,
        "visible": true
    }
}
